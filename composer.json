{
    "name": "cjango/think-sms",
    "description": "cjango sms sdk for thinkphp",
    "keywords": ["cjango", "sms", "sdk"],
    "type": "think-extend",
    "homepage": "https://github.com/cjango/think-sms",
    "license": "MIT",
    "authors": [
        {
            "name": "Jason.Chen",
            "email": "chenjxlg@163.com",
            "homepage": "https://www.cjango.com/"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.6.0",
        "topthink/think-migration": "^1.0",
        "topthink/think-installer": "^1.0"
    },
    "require-dev": {
        "topthink/framework": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "cjango\\": "src/"
        },
        "files": [
            "src/common.php"
        ]
    },
    "scripts": {
        "post-package-install": "MyVendor\\MyClass::postUpdate"
    },
    "extra": {
        "think-config": {
            "sms": "config.php"
        }
    }
}
